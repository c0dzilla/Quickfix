        android:layout_alignStart="@+id/textView3" />

    <TextView
        android:text="TextView"
        android:textColor="#000"
        android:textSize="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView22"
